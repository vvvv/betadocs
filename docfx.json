{
  "build": {
    "content": [
      {
        "files": [
          "**.{md,yml}",
          "**/toc.yml",
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "img/**",
          "**.{jpg,png,jpeg,PNG,JPG,gif,GIF,svg,SVG}"
        ]
      }
    ],
    "dest": "_site",
    "sitemap":{
      "baseUrl": "https://beta.vvvv.org/",
      "priority": 0.1,
      "changefreq": "never"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
    "default",
    "custom_templates/fxstrap",
    "custom_templates/vvvv"
    ],
    "postProcessors": [ "ExtractSearchIndex" ],
	  "globalMetadata": {
      "_appTitle": "vvvv beta documentation",
      "_contributionsTitle": "vvvv beta contributions",
      "_appFooter": "Brought to you by vvvv.org",
      "_enableSearch": true,
      "_enableNewTab": false,
      "_appLogoPath": "/img/icons/logo.svg",
      "_appFaviconPath": "/img/icons/favicon.ico",
      "_disableContribution": false,
      "_disableAffix": false,
      "_gitContribute": {
        "repo": "https://github.com/vvvv/betadocs.git",
        "branch": "master"
      },
      "_gitUrlPattern": "github"
  },
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}